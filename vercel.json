{
  "buildCommand": "cd nextjs-yoshibow_blog && npm run build",
  "outputDirectory": "nextjs-yoshibow_blog/.next",
  "framework": "nextjs",
  "installCommand": "cd nextjs-yoshibow_blog && npm install",
  "rootDirectory": "nextjs-yoshibow_blog",
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap"
    },
    {
      "source": "/robots.txt", 
      "destination": "/robots"
    }
  ]
}